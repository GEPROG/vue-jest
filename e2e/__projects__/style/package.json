{
  "name": "style",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --no-cache test.js"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.0.0-alpha.10",
    "vue": "3.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@vue/test-utils": "^2.0.0-rc.1",
    "jest": "^27.0.0-next.2",
    "postcss": "^7.0.13",
    "sass": "^1.23.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "../../../lib"
    },
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "^~?__styles/(.*)$": "<rootDir>/components/styles/$1"
    },
    "globals": {
      "vue-jest": {
        "resources": {
          "scss": [
            "variables.scss"
          ],
          "less": [
            "variables.less"
          ]
        }
      }
    }
  }
}
